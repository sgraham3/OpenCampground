<div id="explain">
  <%= @prompt.body %>
</div>
<div id="rescanx">
<table cellpadding="0" cellspacing="0"><tr valign="top">
<tr><td>
<%= button_to  I18n.t('reservation.Cancel'), { :action => :abandon_remote },
					      :confirm => I18n.t('remote.Cancel'),
					      :method => "post" %>
</td>
<td>
<%= button_to  I18n.t('remote.StartOver'), { :action => :index } %>
</td>
</table>
</div>
<table cellpadding="0" cellspacing="0"><tr valign="top">
<td>
  <%= render :partial => 'spaces' %>
</td>
<% if defined? @map %>
<td>
  <div id="map">
    <div id="container">
      <img src="<%= @map%>" />
    </div>
  </div>
</td>
<% end %>
</tr></table>
