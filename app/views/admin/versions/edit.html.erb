<% if @success %>
  <p>
  <form method="put" action=<%=admin_version_path(@revision)%>  class="button-to"><div>
  <% if defined?(@maintenance) %>
    <input type="hidden" name="maintenance" value=<%=@maintenance%>  />
  <% else %>
    <input type="hidden" name="revision" value=<%=@revision%> />
  <% end %>
  <input type="submit" value=<%=I18n.t('update.Install')%> /></div></form>
<% else %>
  <%= button_to I18n.t('update.Retry'), admin_versions_path, :method => :get %>
<% end %>
<%= button_to I18n.t('update.Skip'), admin_index_path, :method => :get %>
